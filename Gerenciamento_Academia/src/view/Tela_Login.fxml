      <Button layoutX="441.0" layoutY="292.0" mnemonicParsing="false" onAction="#btnLogar" style="-fx-background-color: aqua; -fx-border-color: black;" text="Entrar" textFill="#364a3d" />
      <PasswordField fx:id="txtPassword" layoutX="300.0" layoutY="237.0" prefHeight="25.0" prefWidth="189.0" promptText="password" style="-fx-background-color: transparent; -fx-border-color: #000000;" />
